<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:SurfRater.Avalonia.ViewModels"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="using:SurfRater.Avalonia.Views"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="SurfRater.Avalonia.Views.MainWindow"
        Icon="/Assets/avalonia-logo.ico"
        Title="SurfRater.Avalonia">
        <!--<views:MainView/>-->
	<StackPanel>
		<StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
			<Button Content="Home" Command="{Binding ShowHomeCommand}" />
			<!--<Button Content="Settings" Command="{Binding ShowHomeCommand}" />-->
			<Button Content="About" Command="{Binding ShowSettingsCommand}" />
		</StackPanel>

		<ContentControl Content="{Binding CurrentViewModel}" />
	</StackPanel>
</Window>
